{
    "name": "@benzed/types",
    "description": "Utility types, type guards and assertions.",
    "main": "./lib",
    "version": "1.5.1",
    "publishConfig": {
        "access": "public"
    },
    "scripts": {
        "update": "npx npm-check -u",
        "test": "jest --run-in-band --coverage --all",
        "test:dev": "jest --run-in-band --only-changed --watch --verbose --coverage",
        "tsc": "shx rm -rf lib && tsc",
        "lint": "eslint src/**/*.ts --fix",
        "build": "npm run test && npm run lint && npm run tsc",
        "prepublishOnly": "npm run build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BenZed/types.git"
    },
    "author": "BenZed",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/BenZed/types/issues"
    },
    "homepage": "https://github.com/BenZed/types#readme",
    "devDependencies": {
        "@benzed/dev": "^0.15.0",
        "@types/jest": "^29.2.0",
        "@types/node": "^20.4.5",
        "@typescript-eslint/eslint-plugin": "^6.2.1",
        "@typescript-eslint/parser": "^6.2.1",
        "eslint": "^8.46.0",
        "eslint-config-prettier": "^8.9.0",
        "eslint-plugin-prettier": "^5.0.0",
        "jest": "^29.6.2",
        "shx": "0.3.4",
        "ts-jest": "^29.0.3",
        "typescript": "^5.1.3"
    }
}
